{
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "browserify src/mwc.js -o docs/public/mwc.js -t babelify",
    "dev": "watchify src/mwc.js -o docs/public/mwc.js -t babelify -v"
